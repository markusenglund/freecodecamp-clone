    "react-dom": "^16.1.0",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-sidebar": "^2.3.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
